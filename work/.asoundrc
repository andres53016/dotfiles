pcm.card0 {
	type hw
	card 0
}
ctl.mixer0 {
	type hw
	card 0
	mmap_emulation true
}
pcm.duplex_sink {
	## This provides software mixing for audio out
	type dmix
	ipc_key 1024
	slave {
		pcm "card0"
		## period_size and buffer_size
		## can be modified to reduce
		## latency or add more 'cusion'
		## through the buffer
		period_time 0
		period_size 1024
		buffer_size 4096
	}
}
pcm.duplex_source {
	## this provides software mixing for audio in
	type dsnoop
	ipc_key 2048
	slave {
		pcm "card0"
		period 0
		period_size 1024
		buffer_size 4096
	}
}
pcm.duplex {
	## this plugin allows you to bind both
	## the dmix and dsnoop plugins together
	type asym
	playback.pcm "duplex_sink"
	capture.pcm "duplex_source"
}
pcm.duplex_plugin {
	## if you do not want to use use
	## mixing by default, you can delete
	## !default entry below and direct
	## apps to use this 'device' as a
	## extra option
	type plug
	slave.pcm "duplex"
}
pcm.dsp0 {
	## this allows most OSS-only apps
	## to use this mixing stuff
	## through the use of the aoss wrapper
	type plug
	slave.pcm "duplex"
}
pcm.mono_source {
	# multiplexing for incoming audio
	type route
	slave.pcm "duplex_source"
	ttable.0.1 1
	ttable.1.0 1
}
pcm.mono_sink {
	# cross-mixing for outgoing mono audio
	type route
	slave.pcm "duplex_sink"
	slave.channels 2
	# ttable.0.1 1
	# ttable.0.0 1
}
pcm.mono {
	type asym
	playback.pcm "mono_sink"
	capture.pcm "mono_source"
}
pcm.mono_plugin {
	type plug
	slave.pcm "mono"
}
ctl.equal {
	type equal
#library "/usr/lib64/ladspa/caps.so"
#	module "Eq"
	library "/usr/lib64/ladspa/mbeq_1197.so"
	module "mbeq"
	channels 2

}
pcm.equal_sink {
	type equal
	slave.pcm "duplex_sink"
#	library "/usr/lib64/ladspa/caps.so"
#	module "Eq"
	library "/usr/lib64/ladspa/mbeq_1197.so"
	module "mbeq"
	channels 2
}
pcm.equal {
	type asym
	playback.pcm "equal_sink"
	capture.pcm "duplex_source"
}
pcm.equal_plugin {
	type plug
	slave.pcm equal
}
ctl.!default {
	type hw
	card 0
}
pcm.!default {
	## this makes alsa apps default
	## to use this config
	type plug
	slave.pcm "duplex"
	#slave.pcm equal
}

