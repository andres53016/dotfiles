set encoding=utf-8
set nobackup
set autoread
set ignorecase
set smartcase
set hlsearch
set incsearch
syntax enable
colorscheme slate
set smarttab
set ai
set si
set cin
set inde=
set showmatch
set spell
set spelllang=en
set spellsuggest=5
set tabstop=4
set softtabstop=4
set shiftwidth=4
"set noexpandtab
command Byztabs setl tabstop=4 softtabstop=4 shiftwidth=4 expandtab
command NoByztabs set tabstop=4 softtabstop=4 shiftwidth=4 noexpandtab
